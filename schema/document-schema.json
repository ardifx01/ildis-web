      "type": "string",
      "format": "date"
    },
    "tanggal_pengundangan": {
      "type": "string",
      "format": "date"
    },
    "jenis": {
      "type": "string"
    },
    "noPeraturan": {
      "type": "string"
    },
    "judul": {
      "type": "string"
    },
    "noPanggil": {
