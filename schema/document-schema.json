      "type": ["string", "null"]
    },
    "abstrak": {
      "type": "string"
    },
    "last_updated": {
      "type": "string",
      "format": "date-time"
    },
    "urlAbstrak": {
      "type": "string",
      "format": "uri"
    },
    "urlDetailPeraturan": {
      "type": "string",
      "format": "uri"
    },
    "fileDownload": {
      "type": "string"
    },
    "urlDownload": {
      "type": "string"
    },
    "subjek": {
      "type": "string"
    },
    "operasi": {
      "type": "string"
    },
    "display": {
      "type": "string"
    }
