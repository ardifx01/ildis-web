    },
    "singkatanJenis": {
      "type": "string"
    },
    "tempatTerbit": {
      "type": "string"
    },
    "penerbit": {
      "type": ["string", "null"]
    },
    "deskripsiFisik": {
      "type": ["string", "null"]
    },
    "sumber": {
      "type": "string"
    },
    "isbn": {
      "type": ["string", "null"]
    },
    "status": {
      "type": "string"
    },
    "bahasa": {
      "type": "string"
    },
    "bidangHukum": {
      "type": "string"
    },
    "teuBadan": {
      "type": ["string", "null"]
    },
